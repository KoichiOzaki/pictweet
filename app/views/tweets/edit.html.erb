<div class="contents row">
  <%= form_tag "/tweets/#{@tweet.id}", method: :patch do %>
    <h3>編集する<h3>
    <input placeholder="image_url" type="text" name="image" value=<%= @tweet.image %>>
    <textarea placeholder="text" name="text" cols="30" row="10"><%= @tweet.text %></textarea>
    <input type="submit" value="SENT">
  <% end %>
</div>